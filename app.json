{
  "expo": {
    "name": "IOTapp",
    "slug": "IOTapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/appLogo/appLogo-2x.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/appLogo/appLogo-2x.png",
        "backgroundColor": "#000"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/appLogo/appLogo-1x.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/appLogo/appLogo-3x.png",
          "imageWidth": 300,
          "resizeMode": "contain",
          "backgroundColor": "#000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}